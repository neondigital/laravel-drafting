{
    "name": "neondigital/laravel-drafting",
    "description": "Drafting for Eloquent models",
    "type": "library",
    "require": {
      "laravel/framework": ">=5.8"
    },
    "require-dev": {
      "phpunit/phpunit": "^8.4@dev",
      "orchestra/testbench": "^5.0@dev"
    },
    "license": "MIT",
    "authors": [
      {
        "name": "Alec Ritson",
        "email": "alec@neondigital.co.uk"
      }
    ],
    "minimum-stability": "dev",
    "autoload": {
      "psr-4": {
        "NeonDigital\\Drafting\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "extra": {
        "laravel": {
            "providers" : [
              "NeonDigital\\Drafting\\DraftingServiceProvider"
            ],
            "aliases": {
              "Versioning": "NeonDigital\\Drafting\\Facade"
            }
        }
    }
  }